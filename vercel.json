{
  "headers": [
    {
      "source": "/(.*).svg",
      "headers" : [
        {
          "key" : "Cache-Control",
          "value" : "public, max-age=86400, must-revalidate"
        }
      ]
    },
    {
      "source": "/(.*).png",
      "headers" : [
        {
          "key" : "Cache-Control",
          "value" : "public, max-age=86400, must-revalidate"
        }
      ]
    },
    {
      "source": "/(.*).css",
      "headers" : [
        {
          "key" : "Cache-Control",
          "value" : "public, max-age=86400, must-revalidate"
        }
      ]
